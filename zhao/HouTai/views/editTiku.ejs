<body>
    <div class="culling-border">
        <div class="left">
            <div class="logo">
                <img src="../images/logo.png">
            </div>
            <div class="jingxuan" onclick="culling()">
                精选
            </div>
            <div class="study" onclick="study()">
                学习
            </div>
            <div class="renwu" onclick="renwu()">
                任务
            </div>
            <div class="danci" onclick="danci()">
                单词
            </div>
            <div class="tiku" onclick="tiku()">
                题库
            </div>
            <div class="qikan" onclick="qikan()">
                期刊
            </div>
            <div class="kecheng" onclick="kecheng()">
                在线课程
            </div>
            <div class="mine" onclick="mine()">
                我的
            </div>
        </div>
        </div>
        <div class="right">
            <form class="zhuce" action="/editTikuAcitve?tId=<%=editTikuList[0].tId%>" method="POST">
                <div class="divv" id="divv">
                    <h3>科目:</h3>
                    <input type="text" name="word" value=<%=editTikuList[0].subject%>>
                </div>
                <div class="divv" id="divv">
                    <h3>题目:</h3>
                    <input type="text" name="content" value=<%=editTikuList[0].content%>>
                </div>
                <div class="divv" id="divv">
                    <h3>用户ID:</h3>
                    <input type="text" name="userID" value=<%=editTikuList[0].userID%>>
                </div>
                <div class="divv" id="divv">
                    <h3>答案:</h3>
                    <input type="text" name="userID" value=<%=editTikuList[0].answer%>>
                </div>
                <div class="divv" id="divv">
                    <h3>题目ID:</h3>
                    <input type="text" name="wordId" value=<%=editTikuList[0].tId%>>
                </div>
                <div><button>确认</button></div>
                
            </form>

        </div>
    </div>
