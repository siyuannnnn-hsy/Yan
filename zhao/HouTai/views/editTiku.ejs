        }
        .right p{
            padding-left: 200px;
            margin-left: 600px;
            padding-top:10px;
            margin-top:10px;
        }
        .table0{
            margin-top:3px;
            margin-left: 250px;
        }
        .serch{
            margin-left: 220px;
            width:600px;
            height: 30px;
        }
        .serch0{
            margin-left: 20px;
        }
        table {
            border-collapse: collapse;
            width: 885px;
            margin-left: 400px;
            /* margin-top:150px; */
            text-align: center;
            display: block;
            margin-top: 10px;
        }
        table td, table th {
            border: 1px solid #cad9ea;
            color: #666;
            width: 1200px;
            height: 45px;
        }
        table thead th {
            background-color: #CCE8EB;
            width: 300px;
        }
        table tr:nth-child(odd) {
            background: #fff;
        }
        table tr:nth-child(even) {
            background: #F5FAFA;
        }
        #foot {
            border-collapse: collapse;
            width: 800px;
            margin: 0 auto;
            text-align: center;
            margin-left: 60px;
            margin-top: 10%;
        }
        #divv{
            width:1200px;
            height: 120px;
        }
        #divv input{
            width:800px;
            height: 80px;
            border-radius: 20px;
        }
