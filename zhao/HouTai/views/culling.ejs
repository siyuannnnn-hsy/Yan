</head>
<body>
    <div class="culling-border">
        <div class="left">
            <div class="logo">
                <img src="../images/logo.png">
            </div>
            <div class="jingxuan" onclick="culling()">
                精选
            </div>
            <div class="tuijian" onclick="tuijian()">
                推荐
            </div>
            <div class="zhengzhi" onclick="zhengzhi()">
                政治
            </div>
            <div class="english" onclick="english()">
                英语
            </div>
            <div class="zuhanyeke" onclick="zhuanyeke()">
                专业课
            </div>
            <div class="study" onclick="study()">
                学习
            </div>
            <div class="mine" onclick="mine()">
                我的
            </div>
        </div>
        <div class="right">
            <div class="header">
                <div class="managerM" onclick="manager()">
                    管理员信息
                </div>
                <div class="userM" onclick="user()">
                    用户信息
                </div>
            </div>
            <table  class="table">
                <thead>
                    <tr>
                        <!-- <th>活动ID</th> -->
                        <th>注意事项</th>
                        <th>科目</th>
                        <th>相关内容</th>
                        <!-- <th></th> -->
                        <th id="action">编辑</th>
                        <!-- <th></th> -->
                        <th id="action">删除</th>
                    </tr>
                </thead>
    
                <tbody id="result"></tbody>
                <%for(var i=0;i < culling.length;i++){%>
                    <tr>
                        <td>
                            <%= culling[i]["Sname"]%>
                        </td>
                        <td>
                            <%= culling[i]["subject"]%>
                        </td>
                        <td>
                            <%= culling[i]["content"]%>
                        </td>
                        <!-- <th></th> -->
                        <th><button>
                            <!-- <a href="/culling?signId=<%=culling[i]["signId"]%>"> -->
                            编辑
                        </button></th>
                        
                        <!-- <th></th> -->
                        <td>
                                <button>
                                    <!-- <a href="/delbm?signId=<%=culling[i]["signId"]%>&&activeId=<%=culling[i]["activeId"]%>"> -->
                                    删除
                                <!-- </a> -->
                                </button>
                        </td>
                    </tr>
                <%}%>
                
    
            </table>
            <div id="foot">
                <p style="float: left;" onclick="showAct()">返回上一页</p>
                <p style="float: right;">确认</p>
            </div>
        </div>
    </div>
</body>
<script>
    function culling() {
        window.location = "http://localhost:3000/culling";
