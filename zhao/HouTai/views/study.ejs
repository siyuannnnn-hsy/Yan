<body>
    <div class="culling-border">
        <div class="left">
            <div class="logo">
                <img src="../images/logo.png">
            </div>
            <div class="jingxuan" onclick="culling()">
                精选
            </div>
            <div class="study" onclick="study()">
                学习
            </div>
            <div class="renwu" onclick="renwu()">
                任务
            </div>
            <div class="danci" onclick="danci()">
                单词
            </div>
            <div class="tiku" onclick="tiku()">
                题库
            </div>
            <div class="qikan" onclick="qikan()">
                期刊
            </div>
            <div class="kecheng" onclick="kecheng()">
                在线课程
            </div>
            <div class="mine" onclick="mine()">
                我的
            </div>
        </div>
        <table  class="table">
            <thead>
                <tr>
                    <!-- <th>活动ID</th> -->
                    <th>学习任务</th>
                    <th>内容</th>
                    <th>学科</th>
                    <!-- <th></th> -->
                    <th id="action">编辑</th>
                    <!-- <th></th> -->
                    <th id="action">删除</th>
                </tr>
            </thead>

            <tbody id="result"></tbody>
            <%for(var i=0;i < study.length;i++){%>
                <tr>
                    <td>
                        <%= study[i]["title"]%>
                    </td>
                    <td>
                        <%= study[i]["content"]%>
                    </td>
                    <td>
                        <%= study[i]["subject"]%>
                    </td>
                    </td>
                    <!-- <th></th> -->
                    <th><button>
                        <!-- <a href="/study?signId=<%=study[i]["signId"]%>"> -->
                        编辑
                    </button></th>
                    
                    <!-- <th></th> -->
                    <td>
                            <button>
                                <!-- <a href="/delbm?signId=<%=study[i]["signId"]%>&&activeId=<%=study[i]["activeId"]%>"> -->
                                删除
                            <!-- </a> -->
                            </button>
                    </td>
                </tr>
            <%}%>
            

        </table>
        <div id="foot">
            <p style="float: left;" onclick="showAct()">返回上一页</p>
            <p style="float: right;">确认</p>
        </div>
    </div>
</body>
