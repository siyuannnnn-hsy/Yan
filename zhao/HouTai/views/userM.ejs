<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>用户信息</title>
    <style type="text/css">
        table {
            border-collapse: collapse;
            width: 885px;
            margin: 0 auto;
            text-align: center;
            display: block;
            margin-top: 100px;
        }
        table td, table th {
            border: 1px solid #cad9ea;
            color: #666;
            height: 45px;
        }
        table thead th {
            background-color: #CCE8EB;
            width: 220px;
        }
        table tr:nth-child(odd) {
            background: #fff;
        }
        table tr:nth-child(even) {
            background: #F5FAFA;
        }

    </style>
</head>
<body>
    <table class="table">
        <thead>
            <tr>
                <!-- <th>活动ID</th> -->
                <th>用户ID</th>
                <th>用户名</th>
                <th>登录密码</th>
                <th>手机号</th>
                <th>用户座右铭</th>
                <th>专业</th>
                <!-- <th></th> -->
                <th id="action">编辑</th>
                <!-- <th></th> -->
                <th id="action">删除</th>
            </tr>
        </thead>
        <tbody id="result"></tbody>
        <%for(var i=0;i < userlist.length;i++){%>
            <tr>
                <td>
                    <%= userlist[i]["userID"]%>
                </td>
                <td>
                    <%= userlist[i]["name"]%>
                </td>
                <td>
                    <%= userlist[i]["userPW"]%>
                </td>
                <td>
                    <%= userlist[i]["iphoneNum"]%>
                </td>
                <td>
                    <%= userlist[i]["motto"]%>
                </td>
                <td>
                    <%= userlist[i]["subject"]%>
                </td>
                <!-- <th></th> -->
                <th>
                    <button><a href="">编辑</a></button>
                </th>
                
                <!-- <th></th> -->
                <td>
                        <button>
                            <a href="">删除</a>
                        </button>
                </td>
            </tr>
        <%}%>
        
    </table>
</body>
<script>

</script>
</html>